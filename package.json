	"type": "module",
	"dependencies": {
		"@inlang/paraglide-js-adapter-sveltekit": "^0.4.0",
		"@supabase/supabase-js": "^2.39.3",
		"bits-ui": "^0.18.0",
		"clsx": "^2.1.0",
		"embla-carousel-svelte": "^8.0.0-rc22",
